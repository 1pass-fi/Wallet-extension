{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        ["module-resolver", {
          "root": ["./src"],
          "alias": {
            "*": "./src/*",
            "shared": "./src/popup/components/shared",
            "components": "./src/popup/components",
            "actions": "./src/popup/actions",
            "stories": "./stories",
            "options/*": "./src/options/*"
          }
        }]
      ]
}
